<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
      
        {% block stylesheets %}
            {# Bootstrap CSS #}
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
			
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />{# Local CSS #}
            <link rel="stylesheet" href="{{ asset('/style/style.css') }}" >
            <link rel="stylesheet" href="{{ asset('/style/partner.css') }}">
            
            <link rel="stylesheet" href="{{ asset('/style/navbar.css') }}">
            <link rel="stylesheet" href="{{ asset('/style/partner.css') }}">
            <link rel="stylesheet" href="{{ asset('/style/contact.css') }}">
            <link rel="stylesheet" href="{{ asset('/style/portfolio.css') }}">
            
        {% endblock %}

        {% block javascripts %}
            {# Bootstrap JS #}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
            <script src="{{ asset('js/navbar.js') }}" defer></script>
            <script src="{{ asset('js/partner.js') }}" defer></script>
            {# <script src="https://kit.fontawesome.com/994dfc7f5c.js" crossorigin="anonymous"></script> #}        
             
        {% endblock %}
    </head>
    <body>
  
    
    <header id="navbar">
        <nav class="navbar-container container">      
            {{ include('component/_navigbar.html.twig') }}
        </nav>

        {% for message in app.flashes('success') %}
        <div class="alert alert-success contact-alert text-center container " role"alert">
            {{ message }}
        </div>
        {% endfor %}
    </header>

        {% block body %}    
        {% endblock %}
     
        
     
      <footer class="footer client-footer d-flex align-items-center">
       {{ include('component/_footer.html.twig') }}   
      </footer>
 
    </body>
</html>
